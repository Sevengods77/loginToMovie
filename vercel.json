{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(api/.*)",
            "dest": "server.js"
        },
        {
            "src": "/login",
            "dest": "public/login.html"
        },
        {
            "src": "/register",
            "dest": "public/register.html"
        },
        {
            "src": "/",
            "dest": "public/register.html"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1",
            "continue": true
        }
    ]
}